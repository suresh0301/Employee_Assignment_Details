@model List<AssignmentEmployeeDetails.Models.EmployeeDemo>
@{
}
<h1>Employee</h1>
<table class ="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>EmailId</th>
            <th>Date Of Birth</th>
            <th>Department</th>
            <th>Salary</th>
            <th>Experience</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.EmailId</td>
                <td>@employee.DateOfBirth.ToString("dd-mm-yyyy")</td>
                <td>@employee.Department</td>
                <td>@employee.Salary</td>
                <td>@employee.experience</td>
                <td>a href="Employee/View/@employee.Id">View</td>


            </tr>
            
        }
    </tbody>
</table>